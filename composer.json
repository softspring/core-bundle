{
    "name": "softspring/core-bundle",
    "description": "This bundle provides common symfony features.",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*",
        "doctrine/orm": "^2.5",
        "symfony/console": "^4.4 || ^5.3 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.3 || ^6.0",
        "symfony/form": "^4.4 || ^5.3 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.3 || ^6.0",
        "symfony/doctrine-bridge": "^4.4 || ^5.3 || ^6.0",
        "symfony/twig-bridge": "^4.4 || ^5.3 || ^6.0",
        "symfony/yaml": "^4.4 || ^5.3 || ^6.0",
        "twig/twig": "^2.0",
        "symfony/webpack-encore-bundle": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CoreBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CoreBundle\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/dependency-injection": ">= 4.0.0 < 4.1.12",
        "symfony/framework-bundle": ">= 4.0.0 < 4.1.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
